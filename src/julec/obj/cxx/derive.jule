use std::jule::sema::{Struct}

fn get_derive_fn_decl_clone(s: &Struct): str {
    let mut obj = gen_struct_kind(s)
    obj += " clone(void) const "
    ret obj
}

fn get_derive_fn_def_clone(s: &Struct): str {
    let mut obj = gen_struct_kind(s)
    obj += " " + obj + "::clone(void) const "
    ret obj
}
