fn main() {
	a := cmplx64(10 + 4i)
	b := cmplx64(8 + 2i)
	r := a + b
	if real(r) != 18 || imag(r) != 6 {
		panic("expected 18+6i")
	}
	if r != 18+6i {
		panic("expected 18+6i")
	}
	a2 := cmplx128(a)
	if a2 != 10+4i {
		panic("expected 10+4i")
	}
	b2 := cmplx128(b)
	if b2 != 8+2i {
		panic("expected 8+2i")
	}
	println(a2)
	println(b2)
}