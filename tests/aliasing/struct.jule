struct Foo {
	x: int
	y: bool
	z: str
}

impl Foo {
	fn test(*self): str { ret "Foo" }
}

type Bar: Foo

impl Bar {
	fn test(*self): str { ret "Bar" }
}

fn init() {
	x := Foo{10, true, "hello"}
	y := Bar(x)
	println(y.x)
	println(y.y)
	println(y.z)
	if x.test() != "Foo" {
		panic("should be Foo")
	}
	if y.test() != "Bar" {
		panic("should be Bar")
	}
}