use "std/fmt"

type Map: map[int]str

impl Bla2 for Map {
	async fn foo2(*self) {
		s := fmt::Sprint(*self)
		if s != "{}" {
			panic("expected {}")
		}
	}
}

async fn mapTest() {
	a := Map{}
	m := Bla2(a)
	m.foo2().await
}