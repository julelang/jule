enum Number: type {
	int,
	uint,
}

type MyNumber: Number

fn init() {
	x := Number(10)
	y := MyNumber(x)
	if x != Number(y) {
		panic("should be equal")
	}
	if MyNumber(x).(int) != Number(y).(int) {
		panic("should be equal")
	}
}