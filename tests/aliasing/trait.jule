trait Trait {
	fn test(self): str
}

struct Foo2{}

impl Trait for Foo2 {
	fn test(self): str { ret "Foo" }
}

type Bar2: Foo2

impl Trait for Bar2 {
	fn test(self): str { ret "Bar" }
}

fn init() {
	let x: Trait = Foo2{}
	let y: Trait = Bar2{}
	if x.test() != "Foo" {
		panic("should be Foo")
	}
	if y.test() != "Bar" {
		panic("should be Bar")
	}
}

struct Boo {
	x: int
}

type Foo3: &Boo

trait Bla {
	fn foo(&self)
}

impl Bla for Foo3 {
	fn foo(&self) {
		if (&Boo)(*self).x != 90 {
			panic("should be 90")
		}
	}
}

trait Bla2 {
	fn foo2(self)
}

impl Bla2 for Foo3 {
	fn foo2(self) {
		if (&Boo)(self).x != 90 {
			panic("should be 90")
		}
	}
}

fn init() {
	f := Bla(new(Foo3, Foo3(new(Boo, {90}))))
	f.foo()
	f2 := Bla2(Foo3(new(Boo, {90})))
	f2.foo2()
}