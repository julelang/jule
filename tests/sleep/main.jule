use "std/math/rand"
use "std/time"

fn main() {
	mut i := 0
	for i < 10; i++ {
		dur := time::Duration(i64(time::Millisecond) * rand::I64N(1000))
		print("Sleeping for ")
		println(dur.Str())
		time::Sleep(dur)
	}
	println("Done!")
}