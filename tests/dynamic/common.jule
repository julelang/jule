enum FooC: type {
	int,
	bool,
}

type Bar: FooC
type Bar2: FooC

enum Baz: type {
	Bar,
	Bar2,
}

fn init() {
	let a: Baz = Bar(20)
	x := a.(Bar)
	if x.(int) != 20 {
		panic("should be 20")
	}
}

fn init() {
	a := Baz(Bar(20))
	x := a.(Bar)
	if x.(int) != 20 {
		panic("should be 20")
	}
}

fn init() {
	a := any(Bar(20))
	x := a.(Bar)
	if x.(int) != 20 {
		panic("should be 20")
	}
}

type FooC2: any
type Bar3: any
type Bar4: any

fn init() {
	a := FooC2(Bar3(20)).(Bar3)
	if a.(int) != 20 {
		panic("should be 20")
	}
	_, ok := a.(Bar4)
	if ok {
		panic("should be false")
	}
}