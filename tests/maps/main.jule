fn main() {
	let mut m: map[i32]str = {
		0: "The",
		1: "Jule",
		2: "Programming",
		3: "Language",
	}
	for key, value in m {
		print(key)
		print(": ")
		println(value)
	}
	println(len(m))
	let mut ok = false
	_, ok = m[0]
	println(ok)
	_, ok = m[10]
	println(ok)
	delete(m, 3)
	delete(m)
	println(len(m))
	_ = map[int]str{
		0: "foo",
		1: "bar",
	}
	_ = map[int]str({
		0: "foo",
		1: "bar",
	})
	_ = make(map[int]str)
	_ = make(map[int]str, 10)
}