use "std/internal/poll"
use "std/sys"

fn pipe()!: (r: &File, w: &File) {
	hr, hw := sys::Pipe()?
	r = newFile(u64(hr), poll::Pipe)?
	w = newFile(u64(hw), poll::Pipe)?
	ret
}