struct timespec {
	tv_sec:  i64
	tv_nsec: i64
}

impl timespec {
	#disable nilptr
	fn setNsec(mut *self, ns: i64) {
		self.tv_sec = ns / 1e9
		self.tv_nsec = ns % 1e9
	}
}