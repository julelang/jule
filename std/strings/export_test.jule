fn StrFind(pattern: str, text: str): int {
	ret strFinder.make(pattern).next(text)
}

fn DumpTables(pattern: str): ([256]int, []int) {
	mut finder := strFinder.make(pattern)
	ret finder.badCharSkip, finder.goodSuffixSkip
}