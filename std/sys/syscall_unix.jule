let STDIN: uintptr = 0
let STDOUT: uintptr = 1
let STDERR: uintptr = 2

fn Rmdir(path: str)! {
	Unlinkat(AT_FDCWD, path, AT_REMOVEDIR)?
}

fn Send(s: int, buf: []byte, flags: int)! {
	Sendto(s, buf, flags, 0, 0)?
}