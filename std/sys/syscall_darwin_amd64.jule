type secType = i64
type usecType = i32

struct Timeval {
	Sec:       i64
	Usec:      i32
	Pad_cgo_0: [4]byte
}

struct Timespec {
	Sec:  i64
	Nsec: i64
}

struct SysStat {
	Dev:           i32
	Mode:          u16
	Nlink:         u16
	Ino:           u64
	Uid:           u32
	Gid:           u32
	Rdev:          i32
	Pad_cgo_0:     [4]byte
	Atimespec:     Timespec
	Mtimespec:     Timespec
	Ctimespec:     Timespec
	Birthtimespec: Timespec
	Size:          i64
	Blocks:        i64
	Blksize:       i32
	Flags:         u32
	Gen:           u32
	Lspare:        i32
	Qspare:        [2]i64
}

struct Dirent {
	Ino:       u64
	Seekoff:   u64
	Reclen:    u16
	Namlen:    u16
	Type:      u8
	Name:      [1024]i8
	Pad_cgo_0: [3]byte
}