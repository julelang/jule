struct bits_error {
	message: str
}

impl Error for bits_error {
	fn error(self): str {
		ret "runtime error: " + self.message
	}
}

static OVERFLOW_ERROR: Error = &bits_error{"integer overflow"}
static DIVIDE_ERROR: Error   = &bits_error{"integer divide by zero"}
