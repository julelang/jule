struct bits_error {
	message: str
}

impl Error for bits_error {
	fn &error() str {
		ret "runtime error: " + .message
	}
}

let overflow_error: Error = &bits_error{"integer overflow"}
let divide_error  : Error = &bits_error{"integer divide by zero"}
