// https://github.com/golang/go/blob/go1.20/src/math/cmplx/rect.go

use "std/math"

// Returns the complex number with polar coordinates r, Î¸.
fn Rect(r: f64, theta: f64): cmplx128 {
	s, c := math::Sincos(theta)
	ret cmplx(r*c, r*s)
}