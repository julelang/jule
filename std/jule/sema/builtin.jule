fn builtinCallerClose(mut &e: &eval, mut &fc: &ast::FuncCallExpr, mut &v: &Value): &Value {
	if len(fc.Args) < 1 {
		e.pushErr(fc.Token, build::LogMsg.MissingExprFor, "c")
		ret nil
	}
	if len(fc.Args) > 2 {
		e.pushErr(fc.Args[1].Token, build::LogMsg.ArgumentOverflow, "close")
	}
	mut dest := e.evalExpr(fc.Args[0])
	match {
	| dest == nil:
		ret nil
	| dest.Decl:
		e.pushErr(fc.Args[0].Token, build::LogMsg.InvalidExpr)
		ret nil
	| dest.Type.Chan() != nil:
		c := dest.Type.Chan()
		if !c.Send {
			e.pushErr(fc.Args[0].Token, build::LogMsg.CloseRecvOnlyChan)
		}
		v = buildVoidValue()
		v.Model = &BuiltinCloseCallExpr{
			Token: fc.Args[0].Token,
			Chan: dest,
		}
		ret v
	|:
		e.pushErr(fc.Args[0].Token, build::LogMsg.InvalidExpr)
		ret nil
	}
}

fn builtinCallerStdComptimeTypeOf(mut &e: &eval, mut &fc: &ast::FuncCallExpr, mut &_: &Value): &Value {
	if len(fc.Args) < 1 {
		e.pushErr(fc.Token, build::LogMsg.MissingExprFor, "type|expr")
		ret nil
	}
	if len(fc.Args) > 1 {
		e.pushErr(fc.Args[1].Token, build::LogMsg.ArgumentOverflow, "TypeOf")
	}

	mut v := e.evalExprKind(fc.Args[0].Kind)
	if v == nil {
		ret nil
	}
	f := v.Type.Func()
	if f != nil {
		if f.IsBuiltin() {
			e.pushErr(fc.Args[0].Token, build::LogMsg.InvalidTypeForTypeOf, "<built-in fn>")
			ret nil
		}
		if len(f.Generics) != len(f.Decl.Generics) {
			e.pushErr(fc.Args[0].Token, build::LogMsg.HasGenerics)
			ret nil
		}
	}
	s := v.Type.Struct()
	if s != nil && len(s.Generics) != len(s.Decl.Generics) {
		e.pushErr(fc.Args[0].Token, build::LogMsg.HasGenerics)
		ret nil
	}
	if v.Type.comptime() {
		e.pushErr(fc.Args[0].Token, build::LogMsg.InvalidTypeForTypeOf, v.Type.Str())
		ret nil
	}
	ret buildComptimeTypeInfoData(e.s, v.Type)
}

fn builtinCallerStdComptimeValueOf(mut &e: &eval, mut &fc: &ast::FuncCallExpr, mut &_: &Value): &Value {
	if len(fc.Args) < 1 {
		e.pushErr(fc.Token, build::LogMsg.MissingExprFor, "expr")
		ret nil
	}
	if len(fc.Args) > 1 {
		e.pushErr(fc.Args[1].Token, build::LogMsg.ArgumentOverflow, "ValueOf")
	}
	mut v := e.evalExpr(fc.Args[0])
	if v == nil {
		ret nil
	}
	// Do not allow value if has unsupported type for type infer.
	if !isGoodValueToInfer(v) {
		e.pushErr(fc.Args[0].Token, build::LogMsg.InvalidTypeForFunc, v.Type.Str(), "ValueOf")
		ret nil
	}
	ret buildComptimeValue(v)
}

