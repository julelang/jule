            ret new(Data)
        }

        match self.op.kind {
        | str(TokenKind.Eqs)
        | str(TokenKind.NotEq):
            ret &Data{
                kind: &TypeKind{
                    kind: build_prim_type(str(PrimKind.Bool)),
                },
            }

        |:
            self.e.push_err(self.op, LogMsg.OperatorNotForJuleType, self.op.kind, self.l.kind.to_str())
